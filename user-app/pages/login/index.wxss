.login-main{
    display: flex;
    flex: 1;
    flex-direction: column;
    align-items: center;
    padding-top: 100rpx;
}
.login-box{
   width:  80%;
   height: 90rpx;
   border: 1px solid #e4e4e4;
   display: flex;
   position: relative;
}
.login-box text{
    font-size: 32rpx;
    line-height: 90rpx;
    position: absolute;
    left: 10px;
    font-weight: bold;
}
.login-input{
    width: 100%;
    height: 100%;
    padding-left: 160rpx;
}
.login-btn{
    width: 80%;
    margin-top: 50rpx;
}
.get-history{
    align-self: flex-start;
    margin-left: 10%;
    margin-top: 4%;
    font-size: 32rpx;
    font-weight: bold;
}